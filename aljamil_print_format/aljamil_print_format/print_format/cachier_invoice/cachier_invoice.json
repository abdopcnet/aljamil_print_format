{
 "absolute_value": 0,
 "align_labels_right": 0,
 "creation": "2025-03-20 21:33:40.499371",
 "css": "",
 "custom_format": 1,
 "default_print_language": "ar",
 "disabled": 0,
 "doc_type": "Sales Invoice",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\r\n\r\n\r\n{# \u0645\u0639\u0627\u0644\u062c\u0629 \u062c\u0647\u0629 \u0627\u0644\u0627\u062a\u0635\u0627\u0644 #}\r\n{% set contact_name = frappe.db.get_value(\"Dynamic Link\", {\"link_doctype\": \"Customer\", \"link_name\": doc.customer, \"parenttype\": \"Contact\"}, \"parent\") %}\r\n{% if contact_name %}\r\n    {% set contact_details = frappe.db.get_value(\"Contact\", contact_name, [\"mobile_no\", \"email_id\"]) %}\r\n    {% set mobile_no = contact_details[0] if contact_details else '' %}\r\n    {% set email_id = contact_details[1] if contact_details else '' %}\r\n{% else %}\r\n    {% set mobile_no = '' %}\r\n    {% set email_id = '' %}\r\n{% endif %}\r\n\r\n    \r\n{% set address_name = frappe.db.get_value(\"Dynamic Link\", {\"link_doctype\": \"Company\", \"link_name\": doc.company, \"parenttype\": \"Address\"}, \"parent\") %}\r\n{% if address_name %}\r\n    {% set address = frappe.db.get_value(\"Address\", address_name, [\"address_line1\", \"address_line2\", \"city\", \"state\", \"country\"]) %}\r\n    {% set address_line11 = address[0] if address else '' %}\r\n    {% set address_line12 = address[1] if address else '' %}\r\n    {% set city1 = address[2] if address else '' %}\r\n    {% set state = address[3] if address else '' %}\r\n    {% set country = address[4] if address else '' %}\r\n{% else %}\r\n    {% set address_line11 = '' %}\r\n    {% set address_line12 = '' %}\r\n    {% set city1 = '' %}\r\n    {% set state = '' %}\r\n    {% set country = '' %}\r\n{% endif %}\r\n\r\n\r\n{% set tax_id = frappe.db.get_value(\"Customer\", doc.customer, \"tax_id\") or '' %}\r\n\r\n<html lang=\"ar\" dir=\"rtl\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>\u0641\u0627\u062a\u0648\u0631\u0629 \u0636\u0631\u064a\u0628\u064a\u0629 \u0645\u0628\u0633\u0637\u0629</title>\r\n    <style>\r\n        @import url('https://fonts.googleapis.com/css2?family=Tajawal:wght@400;700&display=swap');\r\n\r\n        body {\r\n            font-family: 'Tajawal', Arial, sans-serif;\r\n            text-align: right;\r\n            direction: rtl;\r\n            margin: 0;\r\n            padding: 0;\r\n            background: #fff;\r\n        }\r\n\r\n        .invoice-container {\r\n            width: 320px;\r\n            margin: 20px auto;\r\n            padding: 10px;\r\n            border: 1px solid #000;\r\n \r\n        }\r\n\r\n        .header {\r\n            text-align: center;\r\n     \r\n        }\r\n\r\n        .header h2 {\r\n            margin: 5px 0;\r\n            font-size: 16px;\r\n            font-weight: bold;\r\n        }\r\n\r\n.header h3 {\r\n    margin: 5px 0;\r\n    font-size: 14px;\r\n    border: 1px solid #000;\r\n    color: #000000;\r\n    padding: 5px;\r\n    display: inline-block;\r\n    min-width: 80%; /* \u062a\u0648\u0633\u064a\u0639 \u0627\u0644\u0625\u0637\u0627\u0631 \u0642\u0644\u064a\u0644\u064b\u0627 \u0628\u062f\u0648\u0646 \u062c\u0639\u0644\u0647 \u064a\u0623\u062e\u0630 \u0627\u0644\u0639\u0631\u0636 \u0628\u0627\u0644\u0643\u0627\u0645\u0644 */\r\n    text-align: center;\r\n\r\n    /* \u062a\u0638\u0644\u064a\u0644 \u0642\u0648\u064a \u062c\u062f\u064b\u0627 \u0644\u0644\u062c\u0627\u0646\u0628 \u0627\u0644\u0623\u064a\u0645\u0646 \u0648\u0627\u0644\u0623\u0633\u0641\u0644 */\r\n    box-shadow: 6px 6px 0px rgba(0, 0, 0, 1);\r\n}\r\n\r\n\r\n\r\n        .company-info {\r\n            text-align: right;\r\n            font-size: 12px;\r\n            margin-bottom: 10px;\r\n        }\r\n        \r\n.company-info2 {\r\n    text-align: center;\r\n    font-size: 12px;\r\n    margin-bottom: 10px;\r\n\r\n    /* \u0625\u0636\u0627\u0641\u0629 \u062e\u0637 \u0641\u0648\u0642 \u0648\u062a\u062d\u062a \u0627\u0644\u0639\u0646\u0635\u0631 */\r\n    border-top: 1px solid #000;\r\n    border-bottom: 1px solid #000;\r\n    padding: 2px 0; /* \u0625\u0636\u0627\u0641\u0629 \u0628\u0639\u0636 \u0627\u0644\u062a\u0628\u0627\u0639\u062f \u062f\u0627\u062e\u0644 \u0627\u0644\u0625\u0637\u0627\u0631 */\r\n}\r\n\r\n\r\n\r\n        .tables-container {\r\n            display: flex;\r\n            justify-content: space-between;\r\n            gap: 5px;\r\n            margin-top: 10px;\r\n        }\r\n\r\n        /* \u0623\u0646\u0645\u0627\u0637 \u0627\u0644\u062c\u062f\u0627\u0648\u0644 \u0627\u0644\u0639\u0627\u0645\u0629 */\r\n        table {\r\n            width: 100%;\r\n            border-collapse: collapse;\r\n            font-size: 12px;\r\n            margin-top: 10px;\r\n        }\r\n\r\n        td, th {\r\n            border: 1px solid black;\r\n            padding: 5px;\r\n        }\r\n\r\n        th {\r\n\r\n            font-weight: bold;\r\n            text-align: center;\r\n        }\r\n\r\n        /* \u062c\u062f\u0648\u0644 \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a \u0628\u062f\u0648\u0646 \u062d\u062f\u0648\u062f */\r\n        .table-info {\r\n         \r\n            border: none;\r\n            width: 100%;\r\n               font-size: 11px;\r\n        }\r\n        \r\n        .table-info td {\r\n            border: none;\r\n            padding: 3px 5px;\r\n        }\r\n        \r\n        /* \u0627\u0644\u0639\u0645\u0648\u062f \u0627\u0644\u0623\u0648\u0644 (\u0627\u0644\u0639\u0631\u0628\u064a) - \u0645\u062d\u0627\u0630\u0627\u0629 \u0644\u0644\u064a\u0645\u064a\u0646 */\r\n        .table-info td:first-child {\r\n            text-align: right;\r\n        }\r\n        \r\n        /* \u0627\u0644\u0639\u0645\u0648\u062f \u0627\u0644\u062b\u0627\u0646\u064a (\u0627\u0644\u0642\u064a\u0645) - \u0645\u062d\u0627\u0630\u0627\u0629 \u0644\u0644\u0648\u0633\u0637 */\r\n        .table-info td:nth-child(2) {\r\n            text-align: center;\r\n            font-weight: bold;\r\n        }\r\n        \r\n        /* \u0627\u0644\u0639\u0645\u0648\u062f \u0627\u0644\u062b\u0627\u0644\u062b (\u0627\u0644\u0625\u0646\u062c\u0644\u064a\u0632\u064a) - \u0645\u062d\u0627\u0630\u0627\u0629 \u0644\u0644\u064a\u0633\u0627\u0631 */\r\n        .table-info td:last-child {\r\n            text-align: left;\r\n            direction: ltr;\r\n        }\r\n\r\n        /* \u0623\u0646\u0645\u0627\u0637 \u062e\u0627\u0635\u0629 \u0628\u0628\u0627\u0642\u064a \u0627\u0644\u062c\u062f\u0627\u0648\u0644 */\r\n        .table-items {\r\n            \r\n            width: 100%;\r\n           \r\n        }\r\n\r\n        .table-summary {\r\n            width: 48%;\r\n \r\n        }\r\n\r\n        .table-payment {\r\n            width: 48%;\r\n            \r\n        }\r\n\r\n        .table-vision {\r\n            width: 100%;\r\n\r\n        }\r\n\r\n        /* \u062c\u0639\u0644 \u0627\u0631\u062a\u0641\u0627\u0639 \u0627\u0644\u0635\u0641\u0648\u0641 \u0645\u062a\u0633\u0627\u0648\u064a\u064b\u0627 */\r\n        .tables-container tr {\r\n            height: 30px;\r\n        }\r\n\r\n        .qr-section {\r\n            text-align: center;\r\n            margin-top: 10px;\r\n        }\r\n\r\n        .qr-section img {\r\n            width: 120px;\r\n            height: 120px;\r\n        }\r\n\r\n        .footer {\r\n            margin-top: 15px;\r\n            text-align: center;\r\n            font-size: 11px;\r\n            border-top: 1px dashed #000;\r\n            padding-top: 5px;\r\n        }\r\n\r\n        @media print {\r\n            body {\r\n                background: none;\r\n            }\r\n            .invoice-container {\r\n                border: none;\r\n                box-shadow: none;\r\n                width: 100%;\r\n            }\r\n            .table-items th {\r\n              text-align: center;\r\n}\r\n\r\n        }\r\n    </style>\r\n</head>\r\n<body>\r\n\r\n <div class=\"invoice-container\">\r\n    <div class=\"header\">\r\n        <img src=\"{{ frappe.utils.get_url('/files/logo.png') }}\" alt=\"Logo\" style=\"max-width: 150px; height: auto;\">\r\n<h3 style=\"text-align: center; margin: 5px 0;\">\r\n    \u0641\u0627\u062a\u0648\u0631\u0629 \u0636\u0631\u064a\u0628\u064a\u0629 \u0645\u0628\u0633\u0637\u0629 <br> Simplified Tax Invoice\r\n</h3>\r\n <h4>\u0634\u0631\u0643\u0629 \u0646\u0638\u0627\u0631\u0627\u062a \u0627\u0644\u062c\u0645\u064a\u0644</h4>\r\n    </div>\r\n\r\n\r\n        <div class=\"company-info\">\r\n            <p><strong>\u0627\u0644\u0633\u062c\u0644 \u0627\u0644\u062a\u062c\u0627\u0631\u064a:</strong> 101019838</p\r\n            <p><strong>\u0627\u0644\u0631\u0642\u0645 \u0627\u0644\u0636\u0631\u064a\u0628\u064a:</strong> 3105489540003</p>\r\n    \r\n        </div>\r\n        \r\n                <div class=\"company-info2\">\r\n   \r\n            <p><strong></strong>\u0645\u0639\u0631\u0636 \u0627\u0644\u0631\u0628\u0648\u0629</p>\r\n          <p><strong></strong>Al-Rabwa Plaza Tel: 011-4776853 - \u0627\u0644\u0631\u0628\u0648\u0629 \u0628\u0644\u0627\u0632\u0627 </p>\r\n\r\n            \r\n        </div>\r\n        <!-- \u062c\u062f\u0648\u0644 \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a \u0628\u062f\u0648\u0646 \u062d\u062f\u0648\u062f -->\r\n        <table class=\"table-info\">\r\n            <tr>\r\n                <td>\u0631\u0642\u0645 \u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629</td>\r\n                <td><strong>{{doc.name}}</strong></td>\r\n                <td>Invoice No.</td>\r\n            </tr>\r\n            <tr>\r\n                <td>\u0627\u0644\u062a\u0627\u0631\u064a\u062e</td>\r\n                <td><strong>{{doc.posting_date}}</strong></td>\r\n                <td>Date</td>\r\n            </tr>\r\n            <tr>\r\n                <td>\u0631\u0642\u0645 \u0627\u0645\u0631 \u0627\u0644\u0639\u0645\u0644</td>\r\n                <td><strong>{{doc.custom_sales_order}}</strong></td>\r\n                <td>Work Order No.</td>\r\n            </tr>\r\n            <tr>\r\n                <td>\u0627\u0633\u0645 \u0627\u0644\u0639\u0645\u064a\u0644</td>\r\n                <td><strong>{{doc.customer}}</strong></td>\r\n                <td>Customer Name</td>\r\n            </tr>\r\n            <tr>\r\n                <td>\u0631\u0642\u0645 \u0627\u0644\u062c\u0648\u0627\u0644</td>\r\n                <td><strong>{{mobile_no}}</strong></td>\r\n                <td>Customer Mobile</td>\r\n            </tr>\r\n  <tr>\r\n    <td>\u0627\u0644\u0628\u0627\u0626\u0639</td>\r\n    <td><strong>{{ doc.owner }}</strong></td>\r\n    <td>Seller</td>\r\n</tr>\r\n\r\n\r\n            <tr>\r\n                <td>\u0627\u0644\u0631\u0642\u0645 \u0627\u0644\u0636\u0631\u064a\u0628\u064a \u0644\u0644\u0639\u0645\u064a\u0644</td>\r\n                <td><strong>{{tax_id}}</strong></td>\r\n                <td>Customer VAT No.</td>\r\n            </tr>\r\n            <tr>\r\n                <td>\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u062a\u0633\u0644\u064a\u0645</td>\r\n                <td><strong>{{doc.custom_delivery_date}}</strong></td>\r\n                <td>Delivery Date</td>\r\n            </tr>\r\n            <tr>\r\n                <td>\u0634\u0631\u0643\u0629 \u0627\u0644\u062a\u0627\u0645\u064a\u0646</td>\r\n                <td><strong>.....</strong></td>\r\n                <td>Issue Company</td>\r\n            </tr>\r\n        </table>\r\n\r\n        <!-- \u062c\u062f\u0648\u0644 \u0627\u0644\u0623\u0635\u0646\u0627\u0641 -->\r\n  <table class=\"table-items\">\r\n    <tr>\r\n        <th>\u0627\u0644\u0648\u0635\u0641<br>Item Name</th>\r\n        <th>\u0627\u0644\u0643\u0645\u064a\u0629<br>Qty</th>\r\n        <th>\u0627\u0644\u0633\u0639\u0631<br>Price</th>\r\n        <th>\u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a<br>Gross</th>\r\n    </tr>\r\n    {% for row in doc.items %}\r\n    <tr>\r\n        <td style=\"text-align: center;\">{{ row.item_name or '' }}</td>\r\n        <td style=\"text-align: center;\">{{ row.qty or 0 }}</td>\r\n        <td style=\"text-align: center;\">{{ row.rate or 0.00 }}</td>\r\n        <td style=\"text-align: center;\">{{ row.amount or 0.00 }}</td>\r\n    </tr>\r\n    {% endfor %}\r\n</table>\r\n\r\n\r\n        <div class=\"tables-container\">\r\n            <!-- \u062c\u062f\u0648\u0644 \u0627\u0644\u0645\u0644\u062e\u0635 -->\r\n      <table class=\"table-summary\">\r\n    <tr><td>\u0627\u062c\u0645\u0627\u0644\u064a \u0627\u0644\u0645\u0628\u0644\u063a</td><td>{{ \"{:,.2f}\".format(doc.total) }}</td></tr>\r\n    <tr><td>\u0645\u0628\u0644\u063a \u0627\u0644\u0645\u0648\u0627\u0641\u0642\u0629</td><td>{{ doc.custom_approval_amount or \"0.0\" }}</td></tr>\r\n     <tr><td>\u0627\u0644\u062a\u062d\u0645\u064a\u0644</td><td>00.0</td></tr>\r\n    <tr><td>\u0627\u0644\u062e\u0635\u0645</td><td>{{ doc.custom_total_deduction or \"0.00\" }}</td></tr>\r\n    <tr><td>\u0627\u0644\u0645\u0628\u0644\u063a \u0628\u0639\u062f \u0627\u0644\u062e\u0635\u0645</td><td> {{ \"{:,.2f}\".format(doc.net_total) }} </td></tr>\r\n\r\n{% set total_vat = 0 %}\r\n{% for tax in doc.taxes %}\r\n    {% if tax.charge_type in [\"On Net Total\", \"On Previous Row Amount\", \"On Previous Row Total\"] %}\r\n        {% set total_vat = total_vat + tax.tax_amount %}\r\n    {% endif %}\r\n{% endfor %}\r\n\r\n\r\n    <tr><td>\u0636\u0631\u064a\u0628\u0629 \u0627\u0644\u0642\u064a\u0645\u0629 \u0627\u0644\u0645\u0636\u0627\u0641\u0629 (15%)</td><td>{{ \"{:,.2f}\".format(doc.custom_total_taxes) }}</td></tr>\r\n       <tr><td>\u0627\u0644\u0645\u062a\u0628\u0642\u064a</td><td>{{doc.outstanding_amount}}</td></tr>\r\n               <tr><td><strong>\u0635\u0627\u0641\u064a \u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629</strong></td><td><strong>{{ \"{:,.2f}\".format(doc.net_total) }}</strong></td></tr>\r\n</table>\r\n\r\n            <!-- \u062c\u062f\u0648\u0644 \u0627\u0644\u062f\u0641\u0639 -->\r\n    \r\n        <table class=\"table-payment\">\r\n<tr><td>\u0646\u0633\u0628\u0629 \u0627\u0644\u062a\u062d\u0645\u064a\u0644</td><td>{{ doc.custom_insurance_percentage or \"0.00\" }}</td></tr>\r\n\r\n       <tr><td>\u0627\u0644\u062d\u062f \u0627\u0644\u0627\u0639\u0644\u064a</td><td>{{ doc.custom_maximum_limit or \"0.00\" }}</td></tr>\r\n       \r\n    <tr style=\"text-align: center;\">\r\n  <td style=\"font-weight: bold;\">\u0637\u0631\u064a\u0642\u0629 \u0627\u0644\u062f\u0641\u0639</td>\r\n  <td style=\"font-weight: bold;\">\u0627\u0644\u0645\u0628\u0644\u063a</td>\r\n</tr>\r\n\r\n{% set payments = frappe.get_all(\"Payment Entry Reference\",\r\n    filters={\"reference_name\": doc.name, \"reference_doctype\": doc.doctype},\r\n    fields=[\"parent\", \"allocated_amount\"]) %}\r\n\r\n{% set payment_data = {} %}\r\n\r\n{% for p in payments %}\r\n    {% set pe = frappe.get_doc(\"Payment Entry\", p.parent) %}\r\n    {% set mop = pe.mode_of_payment %}\r\n    {% if mop in payment_data %}\r\n        {% set _ = payment_data.update({mop: payment_data[mop] + p.allocated_amount}) %}\r\n    {% else %}\r\n        {% set _ = payment_data.update({mop: p.allocated_amount}) %}\r\n    {% endif %}\r\n{% endfor %}\r\n\r\n<tr>\r\n    <td><strong>\u0646\u0642\u062f\u064b\u0627:</strong></td>\r\n    <td>\r\n        {% if payment_data.get(\"\u0646\u0642\u062f\u064b\u0627\") %}\r\n            {{ \"{:,.2f}\".format(payment_data.get(\"\u0646\u0642\u062f\u064b\u0627\")) }}\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n<tr>\r\n    <td><strong>STC Pay:</strong></td>\r\n    <td>\r\n        {% if payment_data.get(\"STC Pay\") %}\r\n            {{ \"{:,.2f}\".format(payment_data.get(\"STC Pay\")) }}\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n<tr>\r\n    <td><strong>\u0645\u062f\u0649:</strong></td>\r\n    <td>\r\n        {% if payment_data.get(\"\u0645\u062f\u0649\") %}\r\n            {{ \"{:,.2f}\".format(payment_data.get(\"\u0645\u062f\u0649\")) }}\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n<tr>\r\n    <td><strong>\u0641\u064a\u0632\u0627:</strong></td>\r\n    <td>\r\n        {% if payment_data.get(\"\u0641\u064a\u0632\u0627\") %}\r\n            {{ \"{:,.2f}\".format(payment_data.get(\"\u0641\u064a\u0632\u0627\")) }}\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n<tr>\r\n    <td><strong>\u0633\u062f\u0627\u062f:</strong></td>\r\n    <td>\r\n        {% if payment_data.get(\"\u0633\u062f\u0627\u062f\") %}\r\n            {{ \"{:,.2f}\".format(payment_data.get(\"\u0633\u062f\u0627\u062f\")) }}\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n\r\n\r\n\r\n        </table>\r\n        </div>\r\n\r\n        <!-- \u062c\u062f\u0648\u0644 \u0645\u0642\u0627\u0633\u0627\u062a \u0627\u0644\u0646\u0638\u0631 -->\r\n<table class=\"table-vision\">\r\n    <tr>\r\n        <th>\u0645\u0642\u0627\u0633 \u0627\u0644\u0646\u0638\u0631</th>\r\n        <th>SPH</th>\r\n        <th>CYL</th>\r\n        <th>AXIS</th>\r\n        <th>ADD</th>\r\n        <th>PD</th>\r\n    </tr>\r\n    {% for row in doc.custom_size %}\r\n    <tr>\r\n        <td>\u0627\u0644\u0639\u064a\u0646 \u0627\u0644\u064a\u0633\u0631\u0649</td>\r\n        <td>{{ row.sphl or '' }}</td>\r\n        <td>{{ row.cyll or '' }}</td>\r\n        <td>{{ row.axisl or '' }}</td>\r\n        <td>{{ row.addl or '' }}</td>\r\n        <td>{{ row.pdl or '' }}</td>\r\n    </tr>\r\n    <tr>\r\n        <td>\u0627\u0644\u0639\u064a\u0646 \u0627\u0644\u064a\u0645\u0646\u0649</td>\r\n        <td>{{ row.sphr or '' }}</td>\r\n        <td>{{ row.cylr or '' }}</td>\r\n        <td>{{ row.axisr or '' }}</td>\r\n        <td>{{ row.addr or '' }}</td>\r\n        <td>{{ row.pdr or '' }}</td>\r\n    </tr>\r\n    {% endfor %}\r\n</table>\r\n           <p><strong>\u0627\u0644\u0645\u0644\u0627\u062d\u0638\u0627\u062a</strong></p>\r\n\r\n        <div class=\"qr-section\">\r\n            <img src=\"qr-code-placeholder.png\" alt=\"QR Code\">\r\n        </div>\r\n\r\n\r\n<div style=\"font-size: 11px; direction: ltr; text-align: center;\">\r\n    <strong>\r\n        {{ frappe.utils.get_datetime(doc.creation).strftime('%Y-%m-%d') }} |  \r\n        {{ frappe.utils.get_datetime(doc.creation).strftime('%I:%M %p') }} |\r\n        <span>{{ doc.owner }}: \u0627\u0633\u0645 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645</span>